{
  "expo": {
    "name": "DosyaYoneticisi",
    "slug": "DosyaYoneticisi",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaanertugrulkoc.DosyaYoneticisi",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Bu uygulama fotoğraflarınıza erişmek için izin istiyor.",
        "NSPhotoLibraryAddUsageDescription": "Bu uygulama fotoğraf kaydetmek için izin istiyor."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kaanertugrulkoc.DosyaYoneticisi",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}